
config AdGuardHome 'AdGuardHome'
	option httpport '3000'
	option configpath '/etc/config/AdGuardHome.yaml'
	option logfile '/tmp/AdGuardHome.log'
	option waitonboot '0'
	option crontab 'cutquerylog cutruntimelog'
	option redirect 'dnsmasq-upstream'
	option backupfile 'filters'
	option workdir '/etc/config/AdGuardHomeConfig'
	option backupwdpath '/etc/config/AdGuardHomeConfig'
	option binpath '/usr/bin/AdGuardHome'
	option enabled '0'
	option binmtime '0'
